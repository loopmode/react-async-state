{
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        ["transform-runtime", {
          "polyfill": false,
          "regenerator": true
        }]
    ],
    "presets": [
        "react",
        "stage-0",
        ["env", {
            "targets": {
                "browsers": ["last 2 versions", "safari >= 7", "ie >= 11"]
            }
        }]
    ]
}